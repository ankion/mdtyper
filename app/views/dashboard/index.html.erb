<%= render 'layouts/dashboard/toolbar' %>
<div>
  <table class="table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Title</th>
        <th>&nbsp;</th>
      </tr>
    </thead>
    <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.publish_date %></td>
        <td><%= post.title %></td>
        <td><%= link_to 'Edit', edit_blog_post_path(@current_blog, post) %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

